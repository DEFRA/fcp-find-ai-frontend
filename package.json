    "@hapi/inert": "7.1.0",
    "@hapi/vision": "7.0.3",
    "@hapi/yar": "^10.1.1",
    "applicationinsights": "2.8.0",
    "axios": "^1.6.7",
    "dotenv": "^16.0.3",
    "govuk-frontend": "4.7.0",
