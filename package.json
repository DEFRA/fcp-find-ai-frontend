    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "mini-css-extract-plugin": "2.7.6",
    "nodemon": "3.0.1",
    "npm-run-all": "4.1.5",
    "pino-pretty": "^7.5.1",
